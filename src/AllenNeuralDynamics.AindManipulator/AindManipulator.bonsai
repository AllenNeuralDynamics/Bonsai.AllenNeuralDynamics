          <p1:Motor0>0</p1:Motor0>
          <p1:Motor1>0</p1:Motor1>
          <p1:Motor2>0</p1:Motor2>
          <p1:Motor3>0</p1:Motor3>
        </harp:Payload>
      </Expression>
      <Expression xsi:type="MulticastSubject">
        <Name>StepperDriverCommands</Name>
      </Expression>
      <Expression xsi:type="rx:BehaviorSubject" TypeArguments="p2:ManipulatorPosition">
        <rx:Name>MoveRelativePosition</rx:Name>
      </Expression>
      <Expression xsi:type="InputMapping">
        <PropertyMappings>
          <Property Name="Motor0" Selector="X" />
          <Property Name="Motor1" Selector="Y1" />
          <Property Name="Motor2" Selector="Y2" />
          <Property Name="Motor3" Selector="Z" />
        </PropertyMappings>
      </Expression>
      <Expression xsi:type="p1:CreateMessage">
